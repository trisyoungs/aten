importmodel
	name "MDL Mol File"
	nickname mol
	extension mol
	glob *.mol
	
	# Variable declaration
	character line,tit,e
	integer n,natoms,nbonds,ii,jj,bondtype
	double rx,ry,rz
	
	# Create default model
	readline $tit
	newmodel $tit
	
	skipline 2
	
	readline "$natoms $nbonds"
	
	for $n,1,$natoms
		readline "$rx $ry $rz $e"
		newatom $e
		setcoords $rx $ry $rz
	end
	
	for $n,1,$nbonds
		readline "$ii $jj $bondtype"
		newbond $ii $jj $bondtype
	end
	
	finalisemodel
end
	
	
